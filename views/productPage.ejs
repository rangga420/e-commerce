<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>

<body>
  <table>
    <thead>
      <tr>
        <th>Name Product</th>
        <th>Image Product</th>
        <th>Price</th>
        <th>Description</th>
        <th>Stock</th>
      </tr>
    </thead>
    <tbody>
      <% product.forEach(el => { %>
        <tr>
          <td><%= el.nameProduct %></td>
          <td><img src="<%= el.imgProduct %>" alt="photo"></td>
          <td><%= el.price %></td>
          <td><%= el.description %></td>
          <td><%= el.stock %></td>
        </tr>
          <% }) %>
    </tbody>
  </table>
  <!-- loop here tr td -->
</body>

</html>